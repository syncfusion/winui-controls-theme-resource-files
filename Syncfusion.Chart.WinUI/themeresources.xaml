            <SolidColorBrush x:Key="SyncfusionChartAxisMinorTickLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMajorGridLineStroke" Color="{StaticResource ControlStrokeColorDefault}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMinorGridLineStroke" Color="{StaticResource ControlStrokeColorDefault}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisOriginLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartConnectorLineStroke" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartTrackballMarkerStroke" Color="{StaticResource TextFillColorPrimary}"/>
        </ResourceDictionary>

        <ResourceDictionary x:Key="HighContrast">
            <SolidColorBrush x:Key="SyncfusionChartAreaBorderBrush" Color="{ThemeResource SystemColorButtonTextColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartHeaderForeground" Color="{ThemeResource SystemColorWindowTextColor}"/>
