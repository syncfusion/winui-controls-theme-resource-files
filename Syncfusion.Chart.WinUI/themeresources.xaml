<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Syncfusion.Core.WinUI/Themes/Common.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="SyncfusionChartAreaBorderBrush" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartHeaderForeground" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisLabelForeground" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartTrackballLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartCrosshairLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipFill" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipStroke" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipForeground" Color="{StaticResource SystemAltHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballFill" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballStroke" Color="{StaticResource SystemBaseLowColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballForeground" Color="{StaticResource SystemAltHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisCrosshairFill" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisCrosshairStroke" Color="{StaticResource SystemBaseLowColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisCrosshairForeground" Color="{StaticResource SystemAltHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisLabelBorderBrush" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartLegendForeground" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMajorTickLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMinorTickLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMajorGridLineStroke" Color="{StaticResource ControlStrokeColorDefault}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisMinorGridLineStroke" Color="{StaticResource ControlStrokeColorDefault}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisOriginLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartConnectorLineStroke" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartTrackballMarkerStroke" Color="{StaticResource TextFillColorPrimary}"/>
        </ResourceDictionary>
        
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="SyncfusionChartAreaBorderBrush" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartHeaderForeground" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisLabelForeground" Color="{StaticResource TextFillColorPrimary}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartTrackballLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartCrosshairLineStroke" Color="{StaticResource ControlStrongStrokeColorDisabled}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipFill" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipStroke" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartTooltipForeground" Color="{StaticResource SystemAltHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballFill" Color="{StaticResource SystemBaseHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballStroke" Color="{StaticResource SystemBaseLowColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisTrackballForeground" Color="{StaticResource SystemAltHighColor}"/>
            <SolidColorBrush x:Key="SyncfusionChartAxisCrosshairFill" Color="{StaticResource SystemBaseHighColor}"/>
